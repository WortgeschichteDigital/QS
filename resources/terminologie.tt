[% wgd_content = 'true' %]

[% wgd_page_title = 'Wortgeschichte digital: Terminologisches Kerninventar' %]

[% wgd_desc = 'Beschreibung des terminologischen Kerninventars mit Literaturhinweisen und Rückverweisen zu den zum jeweiligen Terminus passenden Wortgeschichten' %]

[% BLOCK html_head %]
