  <link rel="manifest" href="[% base %]wgd.webmanifest">
  <link rel="canonical" href="https://www.zdl.org/wb/wgd/Terminologie">
  <link rel="stylesheet" href="[% base %][% static_file('static/wgd/term.min.css') %]" media="screen">
  <link rel="stylesheet" href="[% base %][% static_file('static/wgd/term-print.min.css') %]" media="print">
  <script src="[% base %][% static_file('static/wgd/term.min.mjs') %]" type="module"></script>
  [% PROCESS 'wb/WGd/_structured-data.tt'
      wgd_bread_url1 = "${base}wb/wortgeschichten/"
      wgd_bread_name1 = 'Wortgeschichte digital'
      wgd_bread_url2 = "${base}wb/wgd/Terminologie"
      wgd_bread_name2 = 'Terminologisches Kerninventar'
      wgd_keywords = '[ "Lexikographie", "Wörterbuch", "Neuhochdeutsch", "Sprache", "Terminologie", "Definition" ]'
      wgd_license = '"license": "https://creativecommons.org/licenses/by-sa/4.0/deed.de",'
  %]
[% END %]

